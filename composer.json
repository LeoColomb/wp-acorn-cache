{
  "name": "leocolomb/wp-acorn-cache",
  "description": "A cache manager powered by Laravel through Acorn.",
  "keywords": [
    "wordpress-dropin",
    "wordpress-cache",
    "cache-object",
    "cache-page",
    "cache-advanced",
    "acorn",
    "laravel",
    "wordpress"
  ],
  "homepage": "https://github.com/LeoColomb/wp-acorn-cache",
  "license": "ISC",
  "authors": [
    {
      "name": "LÃ©o Colombaro",
      "homepage": "https://colombaro.fr/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/installers": "^1.0|^2.0",
    "koodimonni/composer-dropin-installer": "^1.3",
    "roots/acorn": "^v2.0.0-beta.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "wp-cli/wp-cli": "^v2.5"
  },
  "extra": {
    "acorn": {
      "providers": [
        "LeoColomb\\WPAcornCache\\ServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "LeoColomb\\WPAcornCache\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "scripts": {
    "lint": "phpcs",
    "lint:fix": "phpcbf"
  },
  "support": {
    "issues": "https://github.com/LeoColomb/wp-acorn-cache/issues"
  }
}
